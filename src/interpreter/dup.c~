static int	command_prepare(struct s_shell *shell,
				struct s_ast_node_cmd *cmd)
{
  int		fd;

  if (shell->fd[0][0] != STDIN_FILENO)
    {
      dup2(shell->fd[0][0], STDIN_FILENO);
      close(shell->fd[0][1]);
    }
  else
    {
    }
  if (shell->fd[1][1] != STDOUT_FILENO)
    {
      dup2(shell->fd[1][1], STDOUT_FILENO);
      close(shell->fd[1][0]);
    }
  else
    {
    }
  return (0);
}
