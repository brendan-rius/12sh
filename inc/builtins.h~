#ifndef BUILTINS_H_
# define BUILTINS_H_

typedef int (*bi_callback)(int argc, char **argv);

struct s_builtin
{
  int	(*callback)(int argc, char **argv);
};

int	bi_create(struct s_builtin **builtinptr, );

#endif /* !BUILTINS_H_ */
